{
  "name": "appstore-connect-sdk",
  "version": "0.0.7",
  "description": "An App Store Connect API client for Node.js",
  "keywords": [
    "appstore connect",
    "api",
    "sdk",
    "apple"
  ],
  "main": "dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./dist/main.mjs",
      "types": "./dist/main.d.ts"
    },
    "./dist/openapi/apis/*": {
      "require": "./dist/openapi/apis/*.js",
      "import": "./dist/openapi/apis/*.mjs"
    },
    "./dist/openapi/apis": {
      "require": "./dist/openapi/apis/index.js",
      "import": "./dist/openapi/apis/index.mjs"
    },
    "./dist/openapi/models/*": {
      "require": "./dist/openapi/models/*.js",
      "import": "./dist/openapi/models/*.mjs"
    },
    "./dist/openapi/models": {
      "require": "./dist/openapi/models/index.js",
      "import": "./dist/openapi/models/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup src/ --format cjs,esm --dts --minify --clean"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node-fetch": "^2.0.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.2",
    "node-fetch": "^2.0.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0"
  },
  "author": "isaced",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaced/appstore-connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/isaced/appstore-connect-sdk/issues",
    "email": "isaced@163.com"
  },
  "homepage": "https://github.com/isaced/appstore-connect-sdk"
}
